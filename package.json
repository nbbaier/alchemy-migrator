{
	"name": "alchemy-migrator",
	"version": "0.1.0",
	"description": "Migrate Cloudflare Wrangler projects to Alchemy",
	"type": "module",
	"bin": {
		"alchemy-migrator": "./dist/cli/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest --run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"check": "biome check",
		"fix": "biome check --write",
		"lint": "biome lint",
		"format": "biome format --write"
	},
	"dependencies": {
		"commander": "^12.0.0",
		"prompts": "^2.4.2",
		"ora": "^8.0.1",
		"chalk": "^5.3.0",
		"@iarna/toml": "^2.2.5",
		"zod": "^3.22.4",
		"prettier": "^3.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.6",
		"@types/bun": "latest",
		"@types/node": "^20.0.0",
		"@types/prompts": "^2.4.9",
		"@vitest/coverage-v8": "^1.0.0",
		"@vitest/ui": "^4.0.10",
		"fast-check": "^3.15.0",
		"typescript": "^5.3.0",
		"vitest": "^4.0.10"
	},
	"keywords": [
		"alchemy",
		"cloudflare",
		"wrangler",
		"migration",
		"infrastructure-as-code",
		"workers"
	],
	"module": "index.ts",
	"private": true
}
